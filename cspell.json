{
  "version": "0.2",
  "words": [
    "autoprefixer",
    "commitlint",
    "customterm",
    "conventionalcommits",
    "corepack",
    "davelosert",
    "eslintcache",
    "fastify",
    "fnm",
    "fullstack",
    "jsdom",
    "mise",
    "monorepo",
    "mycompany",
    "postcss",
    "secretlintrc",
    "tailwindcss",
    "toml",
    "tsx",
    "typecheck",
    "vitejs",
    "vite",
    "vitest",
    "webdeveric",
    "workspaces"
  ],
  "ignorePaths": [
    ".vscode/",
    ".secretlintrc.json",
    "node_modules",
    "dist",
    "build",
    "coverage",
    "private",
    "package.json",
    "package-lock.json",
    "backend/__tests__/data",
    "frontend/__tests__/data"
  ],
  "languageSettings": [
    {
      "languageId": "commit-msg",
      "ignoreRegExpList": ["/^#.*/gm"]
    }
  ]
}
